{
  "name": "@bluebase/plugin-material-ui",
  "description": "Material UI (web) comes to BlueBase!",
  "author": "BlueEast Team <code@blueeast.com>",
  "version": "1.1.1",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/index.js",
  "repository": "https://github.com/BlueBaseJS/plugin-material-ui",
  "license": "MIT",
  "keywords": [],
  "config": {},
  "size-limit": [
    {
      "limit": "40 KB",
      "webpack": false,
      "path": "dist/**/!(*.test).js",
      "running": false
    }
  ],
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  },
  "scripts": {
    "start": "yarn run storybook",
    "clean": "rimraf coverage dist tmp",
    "watch": "tsc -w -p tsconfig.json",
    "watch:test": "jest --watch",
    "lint": "tslint -t stylish --project \"tsconfig.json\"",
    "info": "npm-scripts-info",
    "build": "run-s clean && run-p build:* && cp -r ./dist/src/* ./dist && rm -r ./dist/src && yarn run size",
    "build:main": "tsc -p tsconfig.json && babel ./src --out-dir dist --ignore \"**/__tests__\",\"**/__stories__\" --extensions \".ts,.tsx,.js,.jsx\"",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:tslint": "tslint --fix --project .",
    "fix:md": "remark . -o",
    "test": "run-s build test:*",
    "test:only": "jest --coverage --forceExit --detectOpenHandles --maxWorkers=10",
    "test:lint": "npm run lint",
    "test:prettier": "prettier \"src/**/*.ts\" --list-different",
    "test:md": "remark . -f",
    "commit": "git-cz",
    "report-coverage": "codecov && cat ./coverage/lcov.info | codacy-coverage",
    "doc": "run-s doc:html && run-s doc:build-storybook && opn docs/index.html",
    "doc:html": "typedoc src/ --target ES6 --mode file --out docs && cp -r assets docs",
    "doc:json": "typedoc src/ --target ES6 --mode file --json docs/typedoc.json && cp -r assets docs",
    "doc:build-storybook": "build-storybook -c ./bluebase/storybook/configs -o docs/storybook",
    "doc:publish": "gh-pages -m \"[ci skip] Updates\" -d docs",
    "reset": "git clean -dfx && git reset --hard && npm i",
    "all": "run-s reset test cov:check doc:html",
    "size": "size-limit",
    "prepublishOnly": "yarn remove react-native",
    "storybook:start": "bluebase storybook:start",
    "storybook-native": "bluebase storybook-native:start",
    "storybook": "start-storybook -p 6006 --config-dir ./bluebase/storybook/configs",
    "screenshot": "storybook-chrome-screenshot -p 9001 -c ./bluebase/storybook/configs",
    "build-storybook": "build-storybook",
    "web:start": "bluebase web:start",
    "expo:start": "bluebase expo:start"
  },
  "scripts-info": {
    "info": "Display information about the package scripts",
    "build": "Clean and rebuild the project",
    "fix": "Try to automatically fix any linting problems",
    "test": "Lint and unit test the project",
    "watch": "Watch and rebuild the project on save",
    "cov": "Rebuild, run tests, then create and open the coverage report",
    "doc": "Generate HTML API documentation and open it in a browser",
    "doc:json": "Generate API documentation in typedoc JSON format",
    "reset": "Delete all untracked files and reset the repo to the last commit"
  },
  "engines": {
    "node": ">=8.9"
  },
  "dependencies": {
    "@bluebase/component-mapper": "^1.4.0",
    "@bluebase/components": "^6.1.0",
    "@material-ui/core": "^4.11.0",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/styles": "^4.10.0",
    "jss-rtl": "^0.3.0",
    "react-native": "^0.61.5",
    "tslib": "^1.11.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.0",
    "@bluebase/cli-essentials": "^2.0.3",
    "@bluebase/code-standards": "^1.3.2",
    "@bluebase/core": "^5.11.1",
    "@bluebase/plugin-vector-icons": "^0.2.0",
    "@bluebase/storybook-addon": "^3.0.0",
    "@expo/webpack-config": "^0.11.9",
    "@material-ui/icons": "^4.9.1",
    "@storybook/addon-a11y": "^5.3.17",
    "@storybook/addon-actions": "^5.3.17",
    "@storybook/addon-info": "^5.3.17",
    "@storybook/addon-knobs": "^5.3.17",
    "@storybook/addon-links": "^5.3.17",
    "@storybook/addon-viewport": "^5.3.17",
    "@storybook/react": "^5.3.17",
    "@types/deepmerge": "^2.2.0",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/enzyme-async-helpers": "^0.9.1",
    "@types/jest": "^25.1.4",
    "@types/react": "^16.9.25",
    "@types/react-dom": "^16.9.5",
    "@types/react-jss": "^10.0.0",
    "@types/react-native": "^0.61.23",
    "@types/react-test-renderer": "^16.9.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.1.0",
    "babel-preset-expo": "^8.0.0",
    "codacy-coverage": "^3.4.0",
    "codecov": "^3.6.5",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-async-helpers": "^0.9.1",
    "jest": "^25.1.0",
    "jest-enzyme": "^7.1.2",
    "jsdom": "^16.2.1",
    "npm-run-all": "^4.1.3",
    "opn-cli": "^5.0.0",
    "prettier": "^2.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-native-typescript-transformer": "^1.2.13",
    "react-test-renderer": "^16.13.1",
    "remark-cli": "^6.0.1",
    "remark-preset-lint-recommended": "^3.0.2",
    "schedule": "^0.5.0",
    "size-limit": "^1.3.1",
    "trash-cli": "^3.0.0",
    "ts-jest": "^25.2.1",
    "tslint": "^5.13.1",
    "typedoc": "^0.17.3",
    "typescript": "3.8.3"
  },
  "peerDependencies": {
    "@bluebase/core": "^4.*",
    "react": "^16.*"
  },
  "prettier": {
    "singleQuote": true,
    "useTabs": true,
    "trailingComma": "es5"
  },
  "publishConfig": {
    "access": "public"
  }
}
