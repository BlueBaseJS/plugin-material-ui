{
  "name": "@bluebase/plugin-material-ui",
  "description": "Material UI (web) comes to BlueBase!",
  "author": "BlueEast Team <code@blueeast.com>",
  "version": "3.8.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/index.js",
  "repository": "https://github.com/BlueBaseJS/plugin-material-ui",
  "license": "MIT",
  "keywords": [],
  "config": {},
  "size-limit": [
    {
      "limit": "40 KB",
      "webpack": false,
      "path": "dist/**/!(*.test).js",
      "running": false
    }
  ],
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  },
  "scripts": {
    "setup": "node ./scripts/setup.js",
    "start": "rnstl && expo start",
    "android": "yarn start --android",
    "ios": "yarn start --ios",
    "web": "yarn start --web",
    "eject": "expo eject",
    "clean": "rimraf coverage dist tmp",
    "build": "run-s clean && run-p build:* && cp -r ./dist/src/* ./dist && rm -r ./dist/src",
    "build:main": "tsc -p tsconfig.json && babel ./src --out-dir dist --ignore \"**/__tests__\",\"**/__stories__\" --extensions \".ts,.tsx,.js,.jsx\"",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:md": "remark . -o",
    "fix:lint": "eslint src --ext=ts,tsx --fix .",
    "test": "run-s build test:*",
    "test:lint": "eslint src --ext=ts,tsx",
    "test:prettier": "prettier \"src/**/*.ts\" --list-different",
    "test:md": "remark . -f",
    "test:only": "jest --coverage --runInBand",
    "storybook-native": "NODE_ENV=STORYBOOK_NATIVE expo start",
    "prestorybook-native": "rnstl",
    "storybook-native-server": "start-storybook -p 7007",
    "storybook": "node ./node_modules/@storybook/react/bin/index.js -p 6006 ",
    "build-storybook": "node ./node_modules/@storybook/react/bin/build.js"
  },
  "dependencies": {
    "@bluebase/component-mapper": "^1.4.0",
    "@bluebase/components": "^8.0.0-alpha.9",
    "@material-ui/core": "^4.12.4",
    "@material-ui/lab": "^4.0.0-alpha.61",
    "@material-ui/styles": "^4.11.5",
    "jss-rtl": "^0.3.0"
  },
  "resolutions": {
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2",
    "graphql": "^16.5.0"
  },
  "devDependencies": {
    "@apollo/client": "^3.5.9",
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.12.9",
    "@bluebase/code-standards": "^2.20.2",
    "@bluebase/core": "^8.0.0-alpha.2",
    "@bluebase/plugin-vector-icons": "^1.1.0",
    "@bluebase/storybook-addon": "^3.0.0",
    "@expo/webpack-config": "~0.16.21",
    "@faker-js/faker": "^6.0.0-alpha.7",
    "@material-ui/icons": "^4.11.3",
    "@storybook/addon-actions": "^5.3",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-info": "^5.3",
    "@storybook/addon-knobs": "^5.3",
    "@storybook/addon-links": "^5.3",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.25",
    "@storybook/node-logger": "^6.3.12",
    "@storybook/react": "^6.3.12",
    "@storybook/react-native": "^5.3.25",
    "@storybook/react-native-server": "^5.3.23",
    "@types/enzyme": "^3.10.10",
    "@types/enzyme-async-helpers": "^0.9.2",
    "@types/faker": "v5.x",
    "@types/jest": "^27.0.2",
    "@types/json-logic-js": "^2.0.1",
    "@types/lodash.camelcase": "^4.3.7",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.kebabcase": "^4.1.7",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.67.6",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.5",
    "babel-loader": "^8.2.3",
    "babel-plugin-istanbul": "^6.1.1",
    "colors": "^1.4.0",
    "enzyme": "^3.11.0",
    "enzyme-async-helpers": "^0.9.1",
    "eslint-plugin-prettier": "^4.0.0",
    "expo": "^45.0.0",
    "faker": "^5.x",
    "jest": "^26.6.3",
    "jest-enzyme": "^7.1.2",
    "jsdom": "^18.0.0",
    "json-logic-js": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.68.2",
    "react-native-storybook-loader": "^2.0.4",
    "react-native-web": "0.17.7",
    "react-test-renderer": "^17.0.2",
    "remark-cli": "^10.0.0",
    "remark-preset-lint-recommended": "^6.1.1",
    "replace-in-file": "^6.3.2",
    "run-all": "^1.0.1",
    "ts-jest": "^26.4.2",
    "typescript": "~4.3.5",
    "waait": "^1.0.5"
  },
  "peerDependencies": {
    "@bluebase/core": "^4.*",
    "react": "^16.*"
  },
  "prettier": {
    "singleQuote": true,
    "useTabs": true,
    "trailingComma": "es5"
  },
  "publishConfig": {
    "access": "public"
  }
}
